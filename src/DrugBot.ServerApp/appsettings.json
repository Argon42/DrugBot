  "AllowedHosts": "*",
  "CustomProcessorsPath": "./Processors/",
  "VkBot.ResetErrorCounter.Seconds": 10,
  "VkBot.ResetErrorCounter.ErrorCount" : 20,
  "VkBot.NeedForward" : false
}
